  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/offchain/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/offchain",
        "command": "node --no-warnings --experimental-vm-modules run-test.js"
      }
    }
  },
  "tags": []
}
