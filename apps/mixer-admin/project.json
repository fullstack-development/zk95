{
  "name": "mixer-admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mixer-admin/src",
  "targets": {
    "build": {
      "executor": "@nx/deno:emit",
      "outputs": [
        "dist/apps/mixer-admin"
      ],
      "options": {
        "main": "apps/mixer-admin/src/main.ts",
        "outputFile": "dist/apps/mixer-admin/main.js",
        "denoConfig": "apps/mixer-admin/deno.json"
      }
    },
    "build-emulation": {
      "executor": "@nx/deno:emit",
      "outputs": [
        "dist/apps/mixer-admin"
      ],
      "options": {
        "main": "apps/mixer-admin/src/emulate.ts",
        "outputFile": "dist/apps/mixer-admin/emulation.js",
        "denoConfig": "apps/mixer-admin/deno.json"
      }
    },
    "deploy": {
      "executor": "@nx/deno:run",
      "options": {
        "buildTarget": "mixer-admin:build",
        "watch": false
      }
    },
    "emulate": {
      "executor": "@nx/deno:run",
      "options": {
        "buildTarget": "mixer-admin:build-emulation",
        "watch": true
      }
    }
  },
  "tags": []
}
